{
  "expo": {
    "name": "Swarna",
    "slug": "Swarna",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/swarna.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "backgroundColor": "#000000",
    "splash": {
      "image": "./assets/images/swarna.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Swarna"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/swarna.png",
        "backgroundColor": "#000000"
      },
      "splash": {
        "image": "./assets/images/swarna.png",
        "resizeMode": "contain",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.Swarna"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/swarna.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/swarna.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#000000"
        }
      ],
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
